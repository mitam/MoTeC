local a = Transmission Brake eq Transmission Brake.Enabled;
local b = Throttle Position eq Throttle Position.Enabled;
local c = Engine Speed eq Engine Speed.Enabled;
local d = Engine Speed Limit eq Engine Speed Limit.Enabled;
local e = Inlet Manifold Pressure eq Inlet Manifold Pressure.Enabled;
local f = Bottle Pressure eq Bottle Pressure.Enabled;

Value = a and b and c and d and e and f
	? Value.Enabled
	: Value.Disabled;
	